{
  "name": "@umoteam/viewer",
  "version": "1.0.3",
  "description": "Umo Viewer is a document viewer built with Vue3. It allows you to preview Umo Editor document content in your browser.",
  "keywords": [
    "umo editor",
    "vue viewer"
  ],
  "homepage": "https://dev.umodoc.com/en/docs/viewer",
  "bugs": {
    "url": "https://github.com/umodoc/viewer/issues",
    "email": "contact@umodoc.com"
  },
  "license": "MIT",
  "author": {
    "name": "umodoc",
    "email": "contact@umodoc.com",
    "url": "https://www.umodoc.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umodoc/viewer.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/umo-viewer.js"
    },
    "./style": {
      "import": "./dist/umo-viewer.css"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 9100 --open --force",
    "format": "oxfmt --write .",
    "lint": "oxlint --fix src",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@umoteam/editor": "latest",
    "@vueuse/core": "^13.5.0",
    "default-passive-events": "^4.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "less": "^4.5.1",
    "less-loader": "^12.3.1",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "tdesign-vue-next": "^1.18.2",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue": "^7.1.1",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.27"
  }
}
